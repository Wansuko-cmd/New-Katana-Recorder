<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_bottom_navigation"
    app:startDestination="@+id/navigation_search">

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.wsr.katanarecorder.main.ui.search.SearchFragment"
        android:label="search_fragment"
        tools:layout="@layout/fragment_search"/>

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.wsr.katanarecorder.main.ui.list.ListFragment"
        android:label="list_fragment"
        tools:layout="@layout/fragment_list"/>

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.wsr.katanarecorder.main.ui.setting.SettingFragment"
        android:label="setting_fragment"
        tools:layout="@layout/fragment_setting"/>

</navigation>